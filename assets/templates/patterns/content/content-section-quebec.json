    "etch-section-style": {
      "type": "element",
      "selector": ":where([data-etch-element=\"section\"])",
      "collection": "default",
      "css": "inline-size: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;",
      "readonly": true
    },
    "xtsiyo4": {
      "type": "class",
      "selector": ".content-section-quebec",
      "collection": "default",
      "css": "background: var(--bg-ultra-light, #fff);\n  /* Begin Content Grid */\n  :where(&) {\n    --feature-width: 50px;\n    --feature-max-width: 100px;\n    --full: minmax(var(--gutter), 1fr);\n    padding-inline: 0;\n    column-gap: 0 !important;\n    display: grid !important;\n    grid-template-columns:\n      [full-start]\n      var(--full)\n      [feature-max-start]\n      minmax(0, var(--feature-max-width, var(--feature-max-width)))\n      [feature-start]\n      minmax(0, var(--feature-width, var(--feature-width)))\n      [content-start]\n      min(var(--content-width), 100% - var(--gutter) * 2)\n      [content-end]\n      minmax(0, var(--feature-width, var(--feature-width)))\n      [feature-end]\n      minmax(0, var(--feature-max-width, var(--feature-max-width)))\n      [feature-max-end]\n      var(--full)\n      [full-end];\n\n    > :not(.content--feature, .content--feature-max, .content--full, .content--full-safe) {\n      max-width: 100%;\n      width: 100%;\n    }\n\n    > :where(:not(.content--feature, .content--feature-max, .content--full, .content--full-safe)) {\n      grid-column: content;\n    }\n\n    > .is-bg {\n      grid-column: full;\n    }\n\n    &:is([class*='section--'], [class*='header--']) {\n      padding-inline: 0;\n    }\n  }\n  /* End Content Grid */\n  @media (width >= to-rem(1280px)) {\n    padding: 0;\n  }",
      "readonly": false
    },
    "etch-container-style": {
      "type": "element",
      "selector": ":where([data-etch-element=\"container\"])",
      "collection": "default",
      "css": "inline-size: 100%;\n  display: flex;\n  flex-direction: column;\n  max-width: var(--content-width, 1366px);\n  align-self: center;",
      "readonly": true
    },
    "3i9nzm1": {
      "type": "class",
      "selector": ".content-section-quebec__inner",
      "collection": "default",
      "css": "display: grid;\n  grid-template-rows: 1fr;\n  grid-column: feature-max;\n  gap: var(--content-gap, 1rem);\n  @media (width >= to-rem(1280px)) {\n    grid-template-columns: minmax(0, 1fr) minmax(0, 2fr);\n    gap: calc(var(--content-gap, 1rem) * 5);\n  }",
      "readonly": false
    },

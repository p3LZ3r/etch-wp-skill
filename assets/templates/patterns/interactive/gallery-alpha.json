    "56mi8g4": {
      "type": "class",
      "selector": ".gallery-alpha",
      "collection": "default",
      "css": "img {\n    aspect-ratio: var(--image-aspect-ratio);\n    object-fit: cover;\n  }\n  button {\n    padding: 0;\n  }\n  /* Begin Auto Grid */\n  & {\n    display: grid !important;\n    grid-template-columns: var(--grid-template-columns);\n    /* Don't touch the stuff below unless you have a PhD in CSS */\n    --gap-count: calc(var(--column-count) - 1);\n    --total-gap-width: calc(var(--gap-count) * var(--grid-gap));\n    --ideal-item-width: calc((100% - var(--total-gap-width)) / var(--column-count));\n    --grid-template-columns: repeat(auto-fit, minmax(max(var(--min-item-width), var(--ideal-item-width)), 1fr));\n    /* Special handling for children */\n    > * {\n      min-width: 0;\n      max-width: 100%;\n      overflow-wrap: break-word;\n    }\n    gap: var(--grid-gap); /* No touchey - redefine this variable if you want to change the gap, but don't replace the variable or your grid will break */\n  }\n  /* End Auto Grid */\n\n  &[data-layout='masonry' i] {\n    img {\n      --image-aspect-ratio: initial;\n    }\n    /* Auto-responsive columns */\n    --col-rule-style: none;\n    --col-rule-width: 1px;\n    --col-rule-color: black;\n\n    display: block !important;\n    columns: var(--column-count) var(--min-item-width);\n    gap: var(--grid-gap);\n\n    > * {\n      break-inside: avoid;\n      margin-block-end: var(--grid-gap);\n    }\n  }\n\n  /* Glightbox */\n .gslide-media {\n  width: auto !important;\n  max-width: 90vw !important;\n}\n\n.gslide-image img {\n  width: auto !important;\n  height: auto !important;\n  max-width: 90vw !important;\n  max-height: 90vh !important;\n  object-fit: contain !important;\n}",
      "readonly": false
    }
  },

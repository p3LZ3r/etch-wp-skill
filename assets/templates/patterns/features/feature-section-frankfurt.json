  "styles": {
    "etch-section-style": {
      "type": "element",
      "selector": ":where([data-etch-element=\"section\"])",
      "collection": "default",
      "css": "inline-size: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;",
      "readonly": true
